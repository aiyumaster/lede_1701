/dts-v1/;

#include "mt7620a.dtsi"

/ {
	compatible = "ralink,mt7620a-dts-board", "ralink,mt7620a-soc";
	model = "Ralink MT7620a DTS board";
	
	chosen {
		bootargs = "console=ttyS1,57600";
	};

	gpio-keys-polled {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio0 2 1>;
			linux,code = <0x100>;
		};
		
	};
};

	&spi0 {
		status = "okay";

		m25p80@0 {
			#address-cells = <1>;
			#size-cells = <1>;
			compatible = "jedec,spi-nor";
			reg = <0>;
			linux,modalias = "m25p80", "en25q64";
			spi-max-frequency = <10000000>;
			m25p,chunked-io = <32>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
				read-only;
			};

			factory: partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				read-only;
			};

			partition@50000 {
				label = "firmware";
				reg = <0x50000 0x1fb0000>;
			};
		};
	};

	&pinctrl {
		state_default: pinctrl0 {
			gpio {
				ralink,group = "i2c";
				ralink,function = "gpio";
			};
		};
	};

	&ethernet {
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&ephy_pins>;
		mtd-mac-address = <&factory 0x4>;
		mediatek,portmap = "llllw";
	};
	
	&wmac {
			ralink,mtd-eeprom = <&factory 0>;
	};
	
	&gsw {
		mediatek,port4 = "gmac";
	};

	&ehci {
		status = "okay";
	};

	&uart {
		status = "okay";
	};

	&uartlite {
		status = "okay";
	};